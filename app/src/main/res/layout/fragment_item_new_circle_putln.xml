    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.ItemNewCirclePutlnFragment">

    <com.wbx.merchant.widget.refresh.PullToRefreshLayout
        android:id="@+id/refresh_layout"
        android:layout_width="match_parent"
